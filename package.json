{
  "name": "howler",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn serve",
    "serve": "node ./build/server.js",
    "build-ts": "tsc",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\"",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon ./build/server.js"
  },
  "dependencies": {
    "@types/mongoose": "^5.2.20",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "mongoose": "^5.3.6",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.0",
    "concurrently": "^4.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.1.3"
  }
}
